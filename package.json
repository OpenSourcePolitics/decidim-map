{
  "name": "decidim-map",
  "description": "#Carto",
  "version": "0.16.0",
  "repository": {
    "url": "git@github.com:OpenSourcePolitics/decidim-map.git",
    "type": "git"
  },
  "author": "moustachu <git@moustachu.net>",
  "license": "AGPL-3.0",
  "engines": {
    "node": "^11.0",
    "npm": "^6.2.0"
  },
  "scripts": {
    "build": "webpack --env.dev --mode=development",
    "build:prod": "NODE_ENV=production webpack -p --env.prod --mode=production",
    "start": "webpack --env.dev --mode=development --watch --progress --color --display-error-details",
    "prelint": "tslint ./**/*.ts ./**/*.tsx",
    "lint": "eslint -c .eslintrc.json --ext .js,.js.es6,.jsx .",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:ci": "npm run lint && npm run test",
    "graphql:download_schema": "apollo service:download --endpoint=http://${DECIDIM_HOST:-localhost:3000}/api",
    "pregraphql:generate_schema_types": "npm run graphql:download_schema",
    "graphql:generate_schema_types": "apollo client:codegen --queries=./**/*.graphql --localSchemaFile=schema.json --target=typescript --globalTypesFile=app/frontend/support/schema.ts"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/codemirror": "0.0.71",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/es6-promise": "^3.3.0",
    "@types/faker": "^4.1.4",
    "@types/jest": "^22.2.3",
    "@types/jquery": "^3.3.29",
    "@types/leaflet": "^1.2.14",
    "@types/leaflet.markercluster": "^1.0.3",
    "@types/prettier": "^1.15.2",
    "@types/pretty-bytes": "^5.1.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-foundation": "^0.9.5",
    "@types/react-icons": "^2.2.6",
    "@types/react-leaflet": "^1.1.6",
    "@types/react-select": "^1.3.4",
    "@types/uuid": "^3.4.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.5",
    "babel-plugin-__coverage__": "~11.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-airbnb": "^2.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.12.0",
    "eslint-plugin-standard": "^3.1.0",
    "expose-loader": "^0.7.5",
    "faker": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.4",
    "json-loader": "~0.5.7",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.11.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "raf": "^3.4.1",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.21.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^4.5.0",
    "tslint": "^5.12.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "typescript-babel-jest": "^1.0.5",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-config-utils": "^2.3.1",
    "yaml-jest": "^1.0.5",
    "yaml-loader": "^0.5.0"
  },
  "dependencies": {
    "@babel/generator": "^7.2.2",
    "@babel/types": "^7.2.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-regular-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "apollo": "^2.1.8",
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link-http": "^1.5.9",
    "axios": "^0.18.0",
    "byte-length": "^1.0.2",
    "classnames": "^2.2.6",
    "codemirror": "^5.42.2",
    "graphql": "^0.13.2",
    "graphql-anywhere": "^4.1.24",
    "graphql-tag": "^2.10.0",
    "i": "^0.3.6",
    "jquery": "^3.2.1",
    "leaflet": "^1.4.0",
    "leaflet.markercluster": "^1.4.1",
    "npm": "^6.5.0",
    "prettier": "1.15.3",
    "pretty-bytes": "^5.1.0",
    "prop-types": "^15.6.2",
    "re-resizable": "^4.11.0",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.7.0",
    "react-foundation": "^0.9.6",
    "react-i18nify": "^1.11.14",
    "react-icons": "^3.2.0",
    "react-leaflet": "^2.1.4",
    "react-leaflet-markercluster": "git://github.com/YUzhva/react-leaflet-markercluster.git#master",
    "react-leaflet-sidetabs": "^1.0.1",
    "react-select": "^1.3.0",
    "react-test-renderer": "^16.7.0",
    "unfetch": "^3.1.2",
    "uuid": "^3.3.2",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(scss|css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "typescript-babel-jest",
      "\\.yml$": "yaml-jest",
      "\\.(gql|graphql)$": "<rootDir>/app/frontend/support/graphql_transformer.js",
      ".*": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "stylelint": {
    "rules": {
      "at-rule-empty-line-before": [
        "always",
        {
          "except": [
            "blockless-after-same-name-blockless",
            "first-nested"
          ],
          "ignore": [
            "after-comment"
          ]
        }
      ],
      "at-rule-name-case": "lower",
      "at-rule-semicolon-newline-after": "always",
      "block-closing-brace-empty-line-before": "never",
      "block-closing-brace-newline-after": "always",
      "block-closing-brace-newline-before": "always-multi-line",
      "block-closing-brace-space-before": "always-single-line",
      "block-no-empty": true,
      "block-opening-brace-newline-after": "always-multi-line",
      "block-opening-brace-space-after": "always-single-line",
      "block-opening-brace-space-before": "never",
      "color-hex-case": "lower",
      "color-hex-length": "short",
      "color-no-invalid-hex": true,
      "comment-empty-line-before": [
        "always",
        {
          "except": [
            "first-nested"
          ],
          "ignore": [
            "stylelint-commands"
          ]
        }
      ],
      "comment-no-empty": true,
      "comment-whitespace-inside": "always",
      "custom-property-empty-line-before": [
        "always",
        {
          "except": [
            "after-custom-property",
            "first-nested"
          ],
          "ignore": [
            "after-comment",
            "inside-single-line-block"
          ]
        }
      ],
      "declaration-bang-space-after": "never",
      "declaration-bang-space-before": "always",
      "declaration-block-no-duplicate-properties": [
        true,
        {
          "ignore": [
            "consecutive-duplicates-with-different-values"
          ]
        }
      ],
      "declaration-block-no-redundant-longhand-properties": true,
      "declaration-block-no-shorthand-property-overrides": true,
      "declaration-block-semicolon-newline-after": "always-multi-line",
      "declaration-block-semicolon-space-after": "always-single-line",
      "declaration-block-semicolon-space-before": "never",
      "declaration-block-single-line-max-declarations": 1,
      "declaration-block-trailing-semicolon": "always",
      "declaration-colon-newline-after": "always-multi-line",
      "declaration-colon-space-after": "always-single-line",
      "declaration-colon-space-before": "never",
      "declaration-empty-line-before": [
        "always",
        {
          "except": [
            "after-declaration",
            "first-nested"
          ],
          "ignore": [
            "after-comment",
            "inside-single-line-block"
          ]
        }
      ],
      "function-calc-no-unspaced-operator": true,
      "function-comma-newline-after": "always-multi-line",
      "function-comma-space-after": "always-single-line",
      "function-comma-space-before": "never",
      "function-linear-gradient-no-nonstandard-direction": true,
      "function-max-empty-lines": 0,
      "function-name-case": "lower",
      "function-parentheses-newline-inside": "always-multi-line",
      "function-parentheses-space-inside": "never-single-line",
      "function-whitespace-after": "always",
      "indentation": 2,
      "keyframe-declaration-no-important": true,
      "length-zero-no-unit": true,
      "max-empty-lines": 1,
      "media-feature-colon-space-after": "always",
      "media-feature-colon-space-before": "never",
      "media-feature-name-case": "lower",
      "media-feature-name-no-unknown": true,
      "media-feature-parentheses-space-inside": "never",
      "media-feature-range-operator-space-after": "always",
      "media-feature-range-operator-space-before": "always",
      "media-query-list-comma-newline-after": "always-multi-line",
      "media-query-list-comma-space-after": "always-single-line",
      "media-query-list-comma-space-before": "never",
      "no-empty-source": true,
      "no-eol-whitespace": true,
      "no-extra-semicolons": true,
      "no-invalid-double-slash-comments": true,
      "no-missing-end-of-source-newline": true,
      "number-leading-zero": "never",
      "number-no-trailing-zeros": true,
      "property-case": "lower",
      "property-no-unknown": true,
      "rule-empty-line-before": [
        "always-multi-line",
        {
          "except": [
            "first-nested"
          ],
          "ignore": [
            "after-comment"
          ]
        }
      ],
      "selector-attribute-brackets-space-inside": "never",
      "selector-attribute-operator-space-after": "never",
      "selector-attribute-operator-space-before": "never",
      "selector-combinator-space-after": "always",
      "selector-combinator-space-before": "always",
      "selector-descendant-combinator-no-non-space": true,
      "selector-list-comma-newline-after": "always",
      "selector-list-comma-space-before": "never",
      "selector-max-empty-lines": 0,
      "selector-pseudo-class-case": "lower",
      "selector-pseudo-class-no-unknown": true,
      "selector-pseudo-class-parentheses-space-inside": "never",
      "selector-pseudo-element-case": "lower",
      "selector-pseudo-element-colon-notation": "double",
      "selector-pseudo-element-no-unknown": true,
      "selector-type-case": "lower",
      "selector-type-no-unknown": true,
      "shorthand-property-no-redundant-values": true,
      "string-no-newline": true,
      "unit-case": "lower",
      "unit-no-unknown": true,
      "value-list-comma-newline-after": "always-multi-line",
      "value-list-comma-space-after": "always-single-line",
      "value-list-comma-space-before": "never",
      "value-list-max-empty-lines": 0
    }
  }
}
